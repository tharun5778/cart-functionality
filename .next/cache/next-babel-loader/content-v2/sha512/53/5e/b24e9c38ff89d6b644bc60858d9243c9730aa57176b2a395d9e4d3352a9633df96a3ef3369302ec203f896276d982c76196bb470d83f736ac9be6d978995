      this.setState({quantity: q})\n      }\n      \n    } \n  }\n\n  increaseQuantity= async (item)=>{\n    if(this.state.quantity<10){\n      if(this.state.isAdded == true){\n        var q = this.state.quantity;\n        var a = q + 1;\n        this.setState({quantity:a});\n        var element = {}\n        element.productID = item.id;\n        element.quantity = a;\n        \n        var config = {\n          method: 'put',\n          url: 'http://localhost:4000/cartItems',\n          headers: { \n            'Content-Type': 'application/json'\n          },\n        };\n        \n        config.data = element;\n        axios(config)\n        .then(function (response) {\n          console.log(JSON.stringify(response.data));\n          \n        })\n        .catch(function (error) {\n          console.log(error);\n          this.setState({quantity:q});\n        });\n      }else{\n        var q = this.state.quantity + 1;\n        this.setState({quantity: q})\n      }\n    }\n  }\n\n  render(){\n    return (\n      <div>\n        <h1>{this.props.item[0].productName}</h1>\n        <h2>{this.props.item[0].description}</h2>\n        <Link href={'/cart'}>\n          <button>\n            go to cart page\n          </button>\n        </Link>\n        <div>\n          <button onClick={()=>this.decreaseQuantity(this.props.item[0])}>\n              -\n          </button>\n        </div>\n        <div>{this.state.quantity}</div>\n        <div>\n          <button onClick={()=>this.increaseQuantity(this.props.item[0])}>\n              +\n          </button>\n        </div>\n        <div>\n          <button onClick={()=>this.add(this.props.item[0])}>\n              add\n          </button>\n        </div>\n        \n        {/* <span onClick={() => router.push('/cart')}>Cart Screen</span> */}\n      </div>\n    )\n  }\n}\n\nexport default Home;"]},"metadata":{},"sourceType":"module"}