    if((this.state.isAdded == false) && (this.state.quantity != 0) ){\n      // console.log(this.state.isAdded)\n      var element = {}\n      element.productID = item.id;\n      element.quantity = this.state.quantity;\n      this.setState({isAdded : true})\n      // var a = JSON.stringify(element)\n      // console.log(a);\n      var config = {\n        method: 'post', \n        url: 'http://localhost:4000/cartItems',\n        headers: { \n          'Content-Type': 'application/json'\n        },\n      };\n      config.data = element;\n      console.log(config);\n      // console.log(this.state.isAdded)\n  \n      axios(config)\n      .then(function (response) {\n        console.log(JSON.stringify(response.data));\n      })\n      .catch(function (error) {\n        console.log(error);\n      });\n    }else{\n      console.log(this.state.isAdded)\n    }\n\n    \n  }\n\n  decreaseQuantity= async (item)=>{\n    if(this.state.quantity>0){\n      if(this.state.isAdded == true){\n        var q = this.state.quantity;\n        var a = q - 1;\n        this.setState({quantity:a});\n        var element = {}\n        element.productID = item.id;\n        element.quantity = a;\n        \n        var config = {\n          method: 'put',\n          url: 'http://localhost:4000/cartItems',\n          headers: { \n            'Content-Type': 'application/json'\n          },\n        };\n        \n        config.data = element;\n        axios(config)\n        .then(function (response) {\n          console.log(JSON.stringify(response.data));\n          \n        })\n        .catch(function (error) {\n          console.log(error);\n          this.setState({quantity:q});\n        });\n      }else{\n        var q = this.state.quantity - 1;\n        this.setState({quantity: q})\n      }\n      \n    } \n  }\n\n  increaseQuantity= async (item)=>{\n    if(this.state.quantity<10){\n      if(this.state.isAdded == true){\n        var q = this.state.quantity;\n        var a = q + 1;\n        this.setState({quantity:a});\n        var element = {}\n        element.productID = item.id;\n        element.quantity = a;\n        \n        var config = {\n          method: 'put',\n          url: 'http://localhost:4000/cartItems',\n          headers: { \n            'Content-Type': 'application/json'\n          },\n        };\n        \n        config.data = element;\n        axios(config)\n        .then(function (response) {\n          console.log(JSON.stringify(response.data));\n          \n        })\n        .catch(function (error) {\n          console.log(error);\n          this.setState({quantity:q});\n        });\n      }else{\n        var q = this.state.quantity + 1;\n        this.setState({quantity: q})\n      }\n    }\n  }\n\n  render(){\n    return (\n      <div style={{backgroundColor:'#edebeb', height: '100vh'}}>\n        <div>\n          <nav class=\"navbar fixed-top navbar-light bg-light\">\n            <div class=\"container-fluid\">\n              <a class=\"navbar-brand\" href=\"#\">Navbar</a>\n            </div>\n          </nav>\n        </div>\n        <div className=\"container-md container-lg\" style={{marginTop:'55px', backgroundColor:'red', height: '100vh', marginTop: '50px',display: 'flex', justifyContent: 'center'}}>\n          <div>\n            <div >\n              <Image\n                src=\"data:image/jpeg;base64,\"\n                alt=\"Picture of the author\"\n                width={300}\n                height={300}\n              />\n            </div>\n          </div>\n          <div>\n          <p>{this.props.item[0].productName}</p>\n            <p>{this.props.item[0].description}</p>\n            <Link href={'/cart'}>\n              <button>\n                go to cart page\n              </button>\n            </Link>\n            <div>\n              <button onClick={()=>this.decreaseQuantity(this.props.item[0])}>\n                  -\n              </button>\n            </div>\n            <div>{this.state.quantity}</div>\n            <div>\n              <button onClick={()=>this.increaseQuantity(this.props.item[0])}>\n                  +\n              </button>\n            </div>\n            <div>\n              <button onClick={()=>this.add(this.props.item[0])}>\n                  add\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n          \n    )\n  }\n}\n\nexport default Home;"]},"metadata":{},"sourceType":"module"}