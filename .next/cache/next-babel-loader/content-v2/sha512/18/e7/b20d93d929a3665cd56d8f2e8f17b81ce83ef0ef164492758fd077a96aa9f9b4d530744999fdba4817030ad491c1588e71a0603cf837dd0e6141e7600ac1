his.setState({quantity:a});\n        var element = {}\n        element.productID = item.id;\n        element.quantity = a;\n        \n        var config = {\n          method: 'put',\n          url: 'http://localhost:4000/cartItems',\n          headers: { \n            'Content-Type': 'application/json'\n          },\n        };\n        \n        config.data = element;\n        axios(config)\n        .then(function (response) {\n          console.log(JSON.stringify(response.data));\n          \n        })\n        .catch(function (error) {\n          console.log(error);\n          this.setState({quantity:q});\n        });\n      }else{\n        var q = this.state.quantity + 1;\n        this.setState({quantity: q})\n      }\n    }\n  }\n\n  render(){\n    return (\n      <div style={{backgroundColor:'#edebeb', height: '100vh'}}>\n        <div>\n          <nav class=\"navbar fixed-top navbar-light bg-light\">\n            <div class=\"container-fluid\">\n              <a class=\"navbar-brand\" href=\"#\">Navbar</a>\n            </div>\n          </nav>\n        </div>\n        <div className=\"container-md container-lg\" style={{marginTop:'55px', backgroundColor:'red', height: '100vh', marginTop: '50px'}}>\n          <div style={{display: 'flex', justifyContent: 'center'}}>\n              <Image\n                src=\"data:image/jpeg;base64,\"\n                alt=\"Picture of the author\"\n                width={300}\n                height={300}\n              />\n              <p>{this.props.item[0].productName}</p>\n            <p>{this.props.item[0].description}</p>\n            <Link href={'/cart'}>\n              <button>\n                go to cart page\n              </button>\n            </Link>\n            <div>\n              <button onClick={()=>this.decreaseQuantity(this.props.item[0])}>\n                  -\n              </button>\n            </div>\n            <div>{this.state.quantity}</div>\n            <div>\n              <button onClick={()=